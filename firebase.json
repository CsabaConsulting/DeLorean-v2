{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "ssrapp"
      }
    ],
    "headers": [{
      "source": "**/*.@(css|jpg|jpeg|png|gif|js|ico|woff)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=864000"
      }]
    }]
  }
}
